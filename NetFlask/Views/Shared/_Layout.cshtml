<body> 
    <!-- header-section-starts -->
    <div class="full">
        <div class="menu">
            <ul>
                <li><a class="@ViewBag.Home" href="/Home/Index"><i class="home"></i></a></li>
                <li><a class="@ViewBag.Videos" href="/Home/Videos"><div class="video"><i class="videos"></i><i class="videos1"></i></div></a></li>
                <li><a class="@ViewBag.Reviews" href="/Home/Reviews"><div class="cat"><i class="watching"></i><i class="watching1"></i></div></a></li>
                <li><a class="@ViewBag.Page404" href="/Home/Page404"><div class="bk"><i class="booking"></i><i class="booking1"></i></div></a></li>
                <li><a class="@ViewBag.Contact" href="/Home/Contact"><div class="cnt"><i class="contact"></i><i class="contact1"></i></div></a></li>
                @if (NetFlask.Infra.SessionUtils.IsLogged)
                {
                    <li><a class="active" title="Logout" href="/Account/Logout"><div class="cat"><i class="Unlock"></i><i class="Unlock1"></i></div></a></li>
                }
                else
                {
                    <li><a title="Login" href="/Account/Login"><div class="cat"><i class="lock"></i><i class="lock1"></i></div></a></li>
                }
